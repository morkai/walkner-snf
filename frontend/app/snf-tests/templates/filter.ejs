<form id="<%- idPrefix %>-form" class="well filter-form">
  <%- forms.dateTimeRange({
    idPrefix: idPrefix,
    property: 'startedAt',
    type: 'date+time',
    startHour: 6,
    labels: {text: helpers.t('PROPERTY:startedAt'), ranges: true}
  }) %>
  <div class="form-group">
    <label><%- helpers.t('PROPERTY:result') %></label>
    <div id="<%- idPrefix %>-result" class="btn-group filter-btn-group" data-toggle="buttons">
      <label class="btn btn-success" title="<%- helpers.t('result:true') %>">
        <input type="checkbox" name="result[]" value="true"> &nbsp;
      </label>
      <label class="btn btn-danger" title="<%- helpers.t('result:false') %>">
        <input type="checkbox" name="result[]" value="false"> &nbsp;
      </label>
    </div>
  </div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= helpers.t('filter:submit') %></span></button>
  </div>
</form>
