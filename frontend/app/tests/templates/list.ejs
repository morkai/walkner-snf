<div class="list">
  <table class="table table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th><%- t('tests', 'PROPERTY:time') %>
        <th><%- t('tests', 'PROPERTY:type') %>
        <th><%- t('tests', 'PROPERTY:user') %>
        <th><%- t('tests', 'PROPERTY:text') %>
      </tr>
    </thead>
    <tbody>
      <% tests.forEach(function(test) { %>
      <tr class="list-item <%- test.severity %>">
        <td><%- test.time %>
        <td><%- test.type %>
        <td>
          <% if (test.user) { %>
            <% if (test.user.login === 'root') { %>
              <%- test.user.login %>
            <% } else { %>
              <a href="#users/<%- test.user._id %>"><%- test.user.login %></a>
            <% } %>
            <% if (test.user.ipAddress) { %>(<%- test.user.ipAddress %>)<% } %>
          <% } else { %>
            -
          <% } %>
        <td><%- test.text %>
      </tr>
      <% }) %>
    </tbody>
  </table>
  <div class="pagination-container"></div>
</div>
